pipeline {
  agent any
  stages {
    stage('stage1') {
      steps {
        echo "Test for jenkinfile"
      }
    }
    stage('stage2') {
      steps {
        echo "The Jenkins file is working !!!"
      }
    }
    stage('stage3') {
      steps {
        echo "Now lets build a real CICD pipline"
      }
    }
  }
}
